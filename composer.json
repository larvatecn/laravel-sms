{
  "name": "larva/laravel-sms",
  "description": "This is a SMS.",
  "keywords": [ "laravel", "sms", "larva" ],
  "type": "library",
  "license": "MIT",
  "require": {
    "ext-json": "*",
    "illuminate/support": "^6.0 || ^7.0 || ^8.0",
    "illuminate/notifications": "^6.0 || ^7.0 || ^8.0",
    "overtrue/easy-sms": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Larva\\Sms\\": "src"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
      "providers": [
        "Larva\\Sms\\SmsServiceProvider"
      ],
      "aliases": {
        "Sms": "Larva\\Sms\\Sms"
      }
    }
  },
  "prefer-stable": true
}
